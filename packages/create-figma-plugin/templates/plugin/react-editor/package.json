{
  "dependencies": {
    "@create-figma-plugin/ui": "^<%- versions.createFigmaPlugin.ui %>",
    "@create-figma-plugin/utilities": "^<%- versions.divriots.createFigmaPluginUtilities %>",
    "preact": ">=10",
    "prismjs": "^1.28.0",
    "react-simple-code-editor": "^0.11.2"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^<%- versions.createFigmaPlugin.build %>",
    "@create-figma-plugin/tsconfig": "^<%- versions.createFigmaPlugin.tsconfig %>",
    "@figma/plugin-typings": "<%- versions.figma.pluginTypings %>",
    "@types/prismjs": "^1.26.0",
    "@types/react": ">=18",
    "react": ">=18",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "<%- id %>",
    "name": "<%- name %>",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
